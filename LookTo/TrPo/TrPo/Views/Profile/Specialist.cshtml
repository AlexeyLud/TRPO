@model TrPo.Models.BecomeSpecialist;

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <link rel="stylesheet" href="/css/auth_reg_help.css">
    <link rel="stylesheet" href="/css/submit.css">
    <title>Стать специалистом</title>
</head>
<body>
    <form asp-action="Index" asp-controller="Home">
        <input class="btn_logo_other" type="image" src="/pictures/logo_small_new.png" alt="LookTo">
    </form>
    <div class="auth_reg_help">
        <h1 align="center"></h1>
        <p></p>
        <form enctype="multipart/form-data" asp-action="Specialist" asp-controller="Profile" asp-antiforgery="true">
            <input type="hidden" name="OurEmail" id="our_email" value="lookto@gmail.com">
            Прикрепите файл с вашими данными <br><input type="file" name="file" id="file" multiple>
            <input type="submit" name="submit" id="submit" class="submit" value="Отправить">
        </form>
    </div>
</body>
</html>